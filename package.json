{
  "name": "gitexp",
  "version": "0.5.0",
  "description": "",
  "main": "dist/gitexp.js",
  "author": "Caian Ertl <hi@caian.org>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caian-org/gitexp.git"
  },
  "bugs": {
    "url": "https://github.com/caian-org/gitexp/issues"
  },
  "homepage": "https://github.com/caian-org/gitexp#readme",
  "scripts": {
    "all": "pnpm run build:js && pnpm run fix:style && pnpm run check:all && pnpm run clean",
    "start": "node dist/gitexp.js",
    "start:ts": "ts-node src/gitexp.ts",
    "build:js": "tsc",
    "build:js:clean": "rm -rf dist && pnpm run build:js",
    "bump:dev": "ncu --upgrade --dep dev --target minor",
    "bump:prod": "ncu --upgrade --dep prod --target minor",
    "bump:all": "pnpm run bump:dev && pnpm run bump:prod",
    "check:lint": "eslint --format=codeframe src/gitexp.ts",
    "check:style": "standardx src/gitexp.ts",
    "check:duplicated": "jscpd",
    "check:all": "pnpm run check:lint && pnpm run check:style && pnpm run check:duplicated",
    "fix:style": "prettier --no-semi --print-width 100 --single-quote --trailing-comma none --write '**/*.ts' && standardx --fix '**/*.ts'",
    "test": "IS_TESTING=1 jest",
    "clean": "rm -rf dist report",
    "commit": "cz",
    "prepare": "[ \"$NODE_ENV\" = \"production\" ] && exit 0; husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "eslintIgnore": [
    "dist",
    "report"
  ],
  "lint-staged": {
    "**/*.ts": [
      "pnpm run fix:style"
    ]
  },
  "jscpd": {
    "threshold": 5,
    "reporters": [
      "html",
      "console"
    ],
    "absolute": true,
    "gitignore": true,
    "ignore": [
      "**/.github/**",
      "**/dist/**",
      "**/report/**"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx,ts,tsx}",
      "!dist/**/*.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 75,
        "functions": 75,
        "branches": 75
      }
    },
    "moduleNameMapper": {
      "^@gitexp/(.*)$": "<rootDir>/src/$1"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/archiver": "^5.3.1",
    "@types/clear": "^0.1.2",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16.11.26",
    "@types/pluralize": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jscpd": "^3.4.5",
    "lint-staged": "^11.2.6",
    "npm-check-updates": "^11.8.5",
    "prettier": "^2.5.1",
    "standardx": "^7.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vrelease-bin": "^0.3.2"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "archiver": "^5.3.0",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "cli-spinners": "^2.6.1",
    "commander": "^8.3.0",
    "date-fns": "^2.28.0",
    "fast-folder-size": "^1.6.1",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "simple-git": "^2.48.0",
    "spinnies-ts": "github:caian-org/spinnies-ts#v0.6.0"
  }
}
