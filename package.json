{
  "name": "github-exporter",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "author": "Caian Ertl <hi@caian.org>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caian-org/github-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/caian-org/github-exporter/issues"
  },
  "engines": {
    "node": "<15",
    "npm": "<7"
  },
  "engineStrict": true,
  "homepage": "https://github.com/caian-org/github-exporter#readme",
  "scripts": {
    "all": "npm run build:js && npm run fix:style && npm run check:all && npm run clean",
    "start": "node dist/index.js",
    "start:ts": "ts-node src/index.ts",
    "build:js": "swc src --out-dir dist & tsc --skipLibCheck --noEmit",
    "build:js:clean": "rm -rf dist && npm run build:js",
    "bump:dev": "ncu --upgrade --dep dev",
    "bump:prod": "ncu --upgrade --dep prod --target minor",
    "bump:all": "npm run bump:dev && npm run bump:prod",
    "check:build": "tsc --noEmit",
    "check:lint": "eslint --format=codeframe '**/*.ts'",
    "check:style": "standardx",
    "check:duplicated": "jscpd",
    "check:all": "npm run check:build && npm run check:lint && npm run check:style && npm run check:duplicated",
    "fix:style": "prettier --no-semi --print-width 100 --single-quote --trailing-comma none --write '**/*.ts' && standardx --fix '**/*.ts'",
    "clean": "rm -rf dist report",
    "commit": "cz",
    "prepare": "[ \"$NODE_ENV\" == \"production\" ] && exit 0; husky install"
  },
  "_moduleAliases": {
    "@ge": "dist"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "eslintIgnore": [
    "dist",
    "report"
  ],
  "lint-staged": {
    "**/*.ts": [
      "npm run fix:style"
    ]
  },
  "jscpd": {
    "threshold": 5,
    "reporters": [
      "html",
      "console"
    ],
    "absolute": true,
    "gitignore": true,
    "ignore": [
      "**/.github/**",
      "**/dist/**",
      "**/report/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@swc/cli": "^0.1.46",
    "@swc/core": "^1.2.74",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.12",
    "@types/nodegit": "^0.27.3",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.1",
    "jscpd": "^3.3.26",
    "lint-staged": "^11.1.1",
    "npm-check-updates": "^11.8.3",
    "prettier": "^2.3.2",
    "standardx": "^7.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "vrelease-bin": "^0.3.1"
  },
  "dependencies": {
    "cli-spinners": "^2.6.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "nodegit": "^0.27.0",
    "octokit": "^1.3.0",
    "ora": "^5.4.1",
    "regenerator-runtime": "^0.13.9"
  }
}
